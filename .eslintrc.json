{
    "plugins": ["unused-imports"],
    "extends": "next/core-web-vitals",
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "no-unused-vars": "off",
        "unused-imports/no-unused-imports": "error",
        "no-console": "warn",
        "eqeqeq": "error",
        "curly": "error",
        "quotes": ["warn", "single"],
        "comma-dangle": ["error", "always-multiline"]
    }
}
